#include <unistd.h>

void ft_putchar(char c)
{
  write(1, &c, 1);
}

void ft_print_alphabet(void)
{
  char letter = 'a';
  while (letter <= 'z')
  {
    ft_putchar(letter);
    letter++;
  }
}

int main (void)
{
  ft_print_alphabet();
  ft_putchar('\n');
  return 0;
}

//Code ft_print_alphabet en c#
